{-# OPTIONS_GHC -Wall #-}
module Golf where

safePick :: Int -> [a] -> Maybe a
safePick _ []     = Nothing
safePick 0 (x:_)  = Just x
safePick k (x:xs) = safePick (k-1) xs

-- | list of lists where the nth-element is the input list with every nth-element
skips :: [a] -> [[a]]
skips xs = undefined

